      <Frame type="Label" name="StateLabel">
        <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-10"/>
        <Style val="SpazzoBottomPanelLockTimer"/>
        <Tooltip val="{$parent/FormatFrame/@Text}"/>
        <AcceptsMouseTooltip val="True"/>
        <TooltipAnchorPosition val="TopRight" />
      </Frame>
      <Frame type="Image" name="StateImage">
        <Anchor side="Top" relative="$parent/StateLabel" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent/StateLabel" pos="Max" offset="0"/>
        <Anchor side="Right" relative="$parent/StateLabel" pos="Min" offset="0"/>
        <Texture val="_Assets\UI\spaz_ui_pause.dds" />
        <Height val="20"/>
        <Width val="20"/>
        <Animation name="Recording">
          <Event event="StartRecord" action="Reset, Play"/>
          <Event event="StopRecord" action="Stop"/>
          <Controller type="Visibility" end="PingPong">
            <Key type="Bool" time="0" value="True"/>
            <Key type="Bool" time="0.5" value="False"/>
          </Controller>
        </Animation>
        <Animation name="Paused">
          <Event event="StopRecord" action="Reset, Play"/>
          <Event event="StartRecord" action="Stop"/>
          <Controller type="Visibility" end="Pause">
            <Key type="Bool" time="0" value="True"/>
            <Key type="Bool" time="0.15" value="True"/>
          </Controller>
        </Animation>
      </Frame>
      <RenderPriority val="513"/>
      <Animation name="RollerOut">
        <Event event="StartRecording" action="Reset,Play" />
        <Controller type="State" stateGroup="RollingOut" end="Pause" frame="$this">
          <Key type="Identifier" time="0" value="RollingOut" />
          <Key type="Identifier" time="0.15" value="RolledOut" />
        </Controller>
      </Animation>
      <StateGroup name="RollingOut">
        <DefaultState val="RolledOut"/>
        <State name="RollingOut"/>
        <State name="RolledOut"/>
      </StateGroup>
      <Animation name="StatLock">
        <Event event="LockStatsAction" action="Reset,Play" />
        <Event event="UnlockStatsAction" action="Stop" />
        <Controller type="State" stateGroup="LockStats" end="Pause" frame="$this">
          <Key type="Identifier" time="0" value="Unlocked" />
          <Key type="Identifier" time="0.15" value="Locked" />
        </Controller>
      </Animation>
      <Animation name="StatUnlock">
        <Event event="UnlockStatsAction" action="Reset,Play" />
        <Controller type="State" stateGroup="LockStats" end="Pause" frame="$this">
          <Key type="Identifier" time="0" value="Unlocked" />
          <Key type="Identifier" time="0.15" value="Unlocked" />
        </Controller>
      </Animation>
      <StateGroup name="LockStats">
        <DefaultState val="Unlocked"/>
        <State name="Unlocked">
          <Action type="SetProperty" frame="$this/StateLabel" Text="@UI/Spazzo/Recording" />
          <Action type="SetProperty" frame="$this/FormatFrame" FormatText="@UI/Spazzo/RecordingInfoTooltip" />
          <Action type="SetProperty" frame="$this/StateImage" Texture="_Assets\UI\spaz_ui_rec.dds" />
          <Action type="SendEvent" frame="$this/StateImage" event="StartRecord" on="Enter" />
        </State>
        <State name="Locked">
          <Action type="SetProperty" frame="$this/StateLabel" Text="@UI/Spazzo/Paused" />
          <Action type="SetProperty" frame="$this/FormatFrame" FormatText="@UI/Spazzo/PausedInfoTooltip" />
          <Action type="SetProperty" frame="$this/StateImage" Texture="_Assets\UI\spaz_ui_pause.dds" />
          <Action type="SendEvent" frame="$this/StateImage" event="StopRecord" on="Enter" />
        </State>
      </StateGroup>
      <Frame type="TextFormatFrame" name="FormatFrame">
        <Anchor relative="$parent" />
        <FormatText val="@UI/Spazzo/RecordingInfoTooltip" />
        <Replacement index="0" val="#TimestampedStatsHotkey" />
        <ReplacedName index="0" val="hotkeyvaluedisplay" />
        <Replacement index="1" val="#PausePlayTimestampHotkey" />
        <ReplacedName index="1" val="hotkeyvaluepause" />
        <Replacement index="2" val="#ResetTimestampHotkey" />
        <ReplacedName index="2" val="hotkeyvaluereset" />
        <Replacement index="3" val="#DisabledTimestampsHotkey" />
        <ReplacedName index="3" val="hotkeyvaluedisable" />
      </Frame>
