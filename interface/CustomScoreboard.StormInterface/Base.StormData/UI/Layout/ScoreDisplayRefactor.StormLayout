  <Frame type="Frame" name="ScoreTemplateFrameVerticalDisplay">
    <Anchor side="Top" relative="$sibling-1" pos="Max" offset="#RefactorScoreContainerButtonGapScores" />
    <Anchor side="Left" relative="$sibling-1" pos="Min" offset="0" />
    <Anchor side="Bottom" relative="$this/ColumnSeparator" pos="Max" offset="0" />
    <Anchor side="Right" relative="$sibling-1" pos="Max" offset="0" />
    <!-- <Visible val="{$sibling-1/@Visible}"/> -->
    <StateGroup name="VisibleStateGroup">
      <DefaultState val="NotVisible"/>
      <State name="Visible">
        <When type="Property" frame="$this/LabelDisplayFrame" operator="NotEqual" Text="Hidden" />
        <Action type="SetProperty" frame="$this" Visible="True" />
      </State>
      <State name="NotVisible">
        <When type="Property" frame="$this/LabelDisplayFrame" operator="Equal" Text="Hidden" />
        <Action type="SetProperty" frame="$this" Visible="False" />
      </State>
    </StateGroup>
    <CollapseLayout val="True"/>
    <PreserveAnchorOffset val="True" />
    <RenderPriority val="600"/>
    <Frame type="DataRefFrame" name="ScoreValueDataReference">
      <Anchor relative="$parent"/>
      <Argument name="score" val="HeroDamage"/>
      <PlayerId val="{$parent/$parent/PlayerNum/@Text}" />
    </Frame>
    <Frame type="CountdownLabel" name="ScoreDisplayLabel">
      <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
      <Anchor side="Left" relative="$parent/LabelDisplayFrame" pos="Max" offset="6" />
      <Anchor side="Bottom" relative="$parent" pos="Max" offset="0" />
      <Style val="Storm_ScoreScreenResultValue" />
      <RenderType val="Offscreen"/>
      <FormatNumber val="True"/>
      <FormatTime val="False"/>
      <CurrentValue val="{$parent/ScoreValueDataReference/@StringValue}"/>
    </Frame>
    <Frame type="CountdownLabel" name="TimeDisplayLabel">
      <Anchor relative="$parent/ScoreDisplayLabel" offset="0" />
      <Style val="Storm_ScoreScreenResultValue" />
      <RenderType val="Offscreen"/>
      <FormatNumber val="False"/>
      <FormatTime val="True"/>
      <CurrentValue val="{$parent/ScoreValueDataReference/@RealValue}"/>
      <Enabled val="False" />
    </Frame>
    <Frame type="Label" name="LabelDisplayFrame">
      <Anchor side="Top" relative="$parent" pos="Min" offset="0" />
      <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
      <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
      <Style val="Storm_ScoreScreenResultValue" />
      <Text val="Hidden" />
    </Frame>
    <Frame type="Label" name="StringValueDisplay">
      <Anchor side="Top" relative="$parent/LabelDisplayFrame" pos="Max" offset="#RefactorFloatingScoreGapBetweenTextAndScore" />
      <Anchor side="Left" relative="$parent/LabelDisplayFrame" pos="Min" offset="0" />
      <Anchor side="Right" relative="$parent/LabelDisplayFrame" pos="Max" offset="0" />
      <Style val="Storm_ScoreScreenResultValue" />
      <Text val="{$parent/ScoreDisplayLabel/@Text}" />
      <StateGroup name="ZeroController">
        <!-- <DefaultState val="Zero"/> -->
        <State name="NonZeroTimer">
          <When type="Property" frame="$parent/TimeDisplayLabel" Enabled="True" />
          <When type="Property" frame="$parent/ScoreValueDataReference" Operator="NotEqual" StringValue="0" />
          <Action type="SetProperty" frame="$this" Text="{$parent/TimeDisplayLabel/@Text}" />
        </State>
        <State name="NonZero">
          <When type="Property" frame="$parent/TimeDisplayLabel" Enabled="False" />
          <When type="Property" frame="$parent/ScoreValueDataReference" Operator="NotEqual" StringValue="0" />
          <Action type="SetProperty" frame="$this" Text="{$parent/ScoreDisplayLabel/@Text}" />
        </State>
        <State name="ZeroTimer">
          <When type="Property" frame="$parent/TimeDisplayLabel" Enabled="True" />
          <When type="Property" frame="$parent/ScoreValueDataReference" StringValue="0" />
          <Action type="SetProperty" frame="$this" Text="0:00" />
        </State>
        <State name="Zero">
          <When type="Property" frame="$parent/TimeDisplayLabel" Enabled="False" />
          <When type="Property" frame="$parent/ScoreValueDataReference" StringValue="0" />
          <Action type="SetProperty" frame="$this" Text="0" />
        </State>
      </StateGroup>
    </Frame>
    <Frame type="Image" name="ColumnSeparator">
      <Anchor side="Top" relative="$parent/StringValueDisplay" pos="Max" offset="2" />
      <Anchor side="Left" relative="$parent" pos="Min" offset="0" />
      <Anchor side="Right" relative="$parent" pos="Max" offset="0" />
      <Anchor side="Bottom" relative="$this" pos="Min" offset="0" />
      <Height val="1"/>
      <RenderPriority val="511" />
      <BlendMode val="Normal" />
      <Color val="8493ff" />
      <AcceptsMouseTooltip val="false" />
      <!-- <PreserveAnchorOffset val="True" /> -->
      <AcceptsMouse val="false" />
      <CollapseLayout val="True"/>
    </Frame>
  </Frame>

