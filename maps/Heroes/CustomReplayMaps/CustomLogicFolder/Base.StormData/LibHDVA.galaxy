    TriggerEnable(libHDVA_gt_DVaAblativeArmorHealingConversion, false);
    TriggerAddDamageEvent(libHDVA_gt_DVaAblativeArmorHealingConversion, null, "HeroDVaMech", c_unitDamageTypeAbility, c_unitDamageNonFatal, null);
