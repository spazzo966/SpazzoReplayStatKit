bool libGDHL_gt_HeroMedicMedivacDropshipEnRouteOrders_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unit lv_dropship;
    int lv_player;
    point lv_medivacPoint;
    unit lv_dropshipBeacon;
    point lv_positionOfBeacon;
