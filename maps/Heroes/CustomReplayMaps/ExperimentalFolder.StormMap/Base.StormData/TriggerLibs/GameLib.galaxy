// Presets
string libGame_ge_BehaviorCategoriesStorm_Ident (int lp_val) {
         if (lp_val == c_behaviorCategoryPermanent) { return "Permanent"; }
    else if (lp_val == c_behaviorCategoryRestorable) { return "ShowUIBar"; }
