fixed libGame_gv_LeechFractionSpecialCaseHeatOfBattle (string lp_damageEffect, fixed lp_damageValue, fixed lp_knownLeechFraction, fixed lp_calculatedLeechFraction, fixed lp_healTakenModifier, unit lp_casterUnit, int lp_casterPlayer) {
	fixed lv_leechFraction;
	fixed lv_leechFractionFromOutside;
	fixed lv_leechFractionValueCombined;
	fixed lv_calculatedLeechFractionResult;
	fixed lv_calculatedLeechFraction;
