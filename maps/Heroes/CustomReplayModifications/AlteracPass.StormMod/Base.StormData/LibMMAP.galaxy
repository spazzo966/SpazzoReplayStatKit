        }

    }

    if ((UnitGroupCount(libMMAP_gv_aVMechanics[lv_teamOfEventPlayer_c].lv_flagEnemiesCapturingUnitGroup, c_unitCountAlive) <= 0)) {
        UnitRemove(libMMAP_gv_aVMechanics[lv_teamOfEventPlayer_c].lv_flagBeingCapturedUnit);
    }

    return true;
}

